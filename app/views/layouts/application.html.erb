<!DOCTYPE html>
<html>
<head>
  <title>Trellino</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<nav class="navbar navbar-default" role="navigation">
	    <div class="container">
	      <div class="navbar-header">
	        <a class="navbar-brand" href="/#">Trellino</a>
	      </div>
	      <ul class="nav navbar-right">
	        <li>
	          <% if logged_in? %>
	            <%= button_to "Log out", session_url, { method: :delete, class: "btn btn-xs btn-warning" } %>
	          <% end %>
	        </li>
	      </ul>
	    </div>
	  </nav>


  <%= yield %>
	

</body>
</html>
